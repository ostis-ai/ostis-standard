@article{Ikeda1998,
	author = {Ikeda, Mitsuru and Seta, Kazuhisa and Kakusho, Osamu and Mizoguchi, Riichiro},
	year = {1998},
	month = {01},
	pages = {126--133},
	title = {Task ontology: Ontology for building conceptual problem solving models},
	booktitle = {ECAI98 Workshop on Applications of ontologies and problem-solving model},
	language = {english},
	shorthand = {Ikeda.M..TaskOntolOBCPSM-1998art},
	annotation = {В работе сделана попытка построения Онтологии задач (Task ontology) и реализации на базе этой онтологии системы концептуального программирования (Conceptual LEvel Programming Environment, CLEPE). Указанная система позволяет (1) описать процесс решения некоторой задачи на уровне удобных и понятных человеку примитивов, (2) промоделировать (сэмулировать) процесс решения задачи на абстрактном уровне в терминах примитивов концептуального уровня. Кроме того, указанная система позволяет пользователю разработать свою онтологию задач, исходя из собственных потребностей.\par
	В работе противопоставляются два вида онтологий задач --
	Обобщенная Онтология задач (core task ontology, C-Task), которая включает в себя понятия, общие для всех предметных областей, а именно описание условия задачи (цель, ограничения), общие классы действий (назначить, выбрать, классифицировать и т.д.), состояния процесса решения задачи и т.д., и частные онтологии задач, описывающие классы задач, решаемые в рамках конкретных предметных областей (task specific ontology, Task-S). Обобщенная Онтология задач, с другой стороны, противопоставляется более высокоуровневым онтологиям (general ontology).\par
	В основе системы CLEPE лежит язык описания онтологий задач (Task Ontology representation Language, TOL), построенный на основе C-Task и позволяющий пользователю разработать онтологию задач для своей предметной области.\par
	Основное внимание в работе уделяется практической стороне вопроса, то есть возможности описания процесса решения задачи на языке, понятном пользователю и близком к естественному, а также возможности моделирования процесса решения задач на абстрактном уровне.}
}

@MISC{Studer1996,
	author = {Studer, Rudi and Eriksson, Henrik and Gennari, John and Tu, Samson and Fensel, Dieter and Musen, Mark},
	year = {1996},
	month = {11},
	pages = {},
	title = {Ontologies and the Configuration of Problem-Solving Methods},
	shorthand = {Studer.R..OntologCPSM-1996art},
	annotation = {В работе говорится о необходимости создания библиотеки многократно используемых методов решения задач, упоминается проект PROTÉGÉ-II, включающий библиотеку таких многократно используемых методов. кратко говорится о структуре библиотеки многократно используемых компонентов систем, основанных на знаниях, о необходимости поиска нужного компонента в библиотеке, основное внимание уделяется проблеме конфигурирования компонентов.\par
	Формулируется проблема совместимости многократно используемых методов в рамках единой системы, которая решается вручную разработчиком. Для упрощения процесса согласования (конфигурирования) множества многократно используемых методов предлагается использовать иерархию онтологий, описывающих методы решения задач различных классов и их подметоды (методы решения подзадач исходной задачи).\par
	Метод делится на подзадачи (явно сказано, что задача и метод ее решения не противопоставляются), каждая из которых, в свою очередь, может делиться на более простые, вплоть до таких подзадач, для которых существует тривиальный ("зашитый") способ их решения. Такой способ в работе назван \textit{элементарным логическим действием} (elementary inference action).\par
	Тем не менее, в работе не дается описания какой-либо предметно-независимой общей онтологии методов в виде семейства классов и отношений. Предложенный подход иллюстрируется на примере онтологии для конкретного класса задач (простая настольная игра в виде поля с клетками, по которому перемещается фишка), формулируется проблема сопоставления (mapping) методов и решаемых ими классов задач. Говорится, что для описания онтологий используется формальный язык KARL (описан, например, в работе \scncite{Fensel1998KARL}), но не приводятся примеры описаний.\par
	Основным результатом работы авторы считают введение понятия онтологии общего метода решения задач и онтологий подзадач (подметодов), что позволяет локализовать внимание разработчика на соответствующих частных онтологиях. Таким образом, речь в работе идет не о некоторой общей онтологии методов, а о самой идее разработки онтологий, соответствующих методам решения задач и формирования иерархических систем таких взаимосвязанных онтологий, соответствующих методам и их подметодам.}}
	
