@article{Duchi2011,
	title        = {Adaptive subgradient methods for online learning and stochastic optimization},
	author       = {Duchi, J. and Hazan, E. and Singer, Y.},
	year         = 2011,
	journal      = {Journal of Machine Learning Research},
	shorthand    = {Duchi J..AdaptSMfOL-2011art},
	annotation   = {The paper presents a new family of subgradient methods that incorporate knowledge of the data's geometry observed in earlier iterations for more informative gradient-based learning. The authors describe an apparatus for adaptively modifying the proximal function and give several efficient algorithms for empirical risk minimization problems with common regularization functions and domain constraints. They show through experimental studies that the adaptive subgradient methods outperform state-of-the-art, non-adaptive subgradient algorithms. The text concludes that their paradigm allows for finding predictive but rarely seen features and simplifies setting a learning rate, resulting in regret guarantees as good as the best proximal function chosen in hindsight}
}

