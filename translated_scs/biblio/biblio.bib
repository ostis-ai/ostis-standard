@inproceedings{Qian2020,
	title        = {Ontological Approach for Chinese Language Interface Design},
	author       = {Qian, Longwei and Sadouski, Mikhail and Li, Wenzu},
	year         = 2020,
	booktitle    = {Communications in Computer and Information Science},
	publisher    = {Springer International Publishing},
